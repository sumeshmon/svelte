{
    "name": "mastertemplate",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "config:env": "sh env.config.sh",
        "dev": "vite dev --port 8080",
        "build": "npm run config:env && vite build",
        "preview": "vite preview --port 8080",
        "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
        "test": "vitest run",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "fix": "eslint . --fix",
        "format": "prettier . --check --plugin-search-dir . --write .",
        "format:changed": "git diff --name-only --diff-filter=MA | xargs prettier --write",
        "prepare": "husky install",
        "pretest": "npm run check",
        "prelint": "npm run check",
        "prebuild": "npm run check",
        "predev": "npm run config:env && npm run check",
        "prepreview": "npm run check",
        "start-php-server": "npm run config:env && vite build && php -S localhost:8080 -t build",
        "test-live-edit": "npm run config:env && node live-edit/index.js"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-static": "^3.0.0",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "bootstrap": "^5.3.3",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte": "^2.30.0",
        "husky": "^8.0.3",
        "jquery": "^3.7.1",
        "jquery-visible": "^1.2.0",
        "path": "^0.12.7",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.10.1",
        "sass": "^1.69.4",
        "slick-carousel": "^1.8.1",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.3",
        "svelte-intersection-observer": "^1.0.0",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "vite-plugin-sass-glob-import": "^3.0.0",
        "vite-plugin-static-copy": "^1.0.3",
        "vitest": "^1.0.0"
    },
    "type": "module",
    "dependencies": {
        "@eqs/ckeditor-lib": "^4.6.0",
        "@eqs/cms-svelte-irtools": "^2.7.8",
        "@eqs/dflip-lib": "^2.3.48",
        "@jill64/sveltekit-adapter-aws": "^1.9.0",
        "@types/gtag.js": "^0.0.19",
        "@types/jquery": "^3.5.30",
        "crypto-js": "^4.2.0",
        "date-fns": "^2.30.0",
        "date-picker-svelte": "^2.13.0",
        "dompurify": "^3.1.7",
        "glob": "^10.3.10",
        "highcharts": "^11.3.0",
        "html2pdf.js": "^0.10.2",
        "iframe-resizer": "^4.3.9",
        "selectric": "^1.13.0",
        "slugify": "^1.6.6",
        "sveltekit-i18n": "^2.4.2",
        "swiper": "^11.1.0",
        "uuid": "^10.0.0"
    }
}
